<section class="products__table">
  <h1 class="titulo">Carts</h1>
  <div class="table__categories">
    {{#each carts}}
      <article class="categories__item">
        <div class="item__texto">
          <div class="id">Cart Id: {{this._id}}</div>
          {{#each products}}
            <div class="price">Product Id: {{this.id._id}}</div>
            <div class="price">Quantity: {{this.quantity}}</div>
          {{/each}}
        </div>
          <a href="http://localhost:8080/carts/{{this._id}}" target="_blank">
            <button>Detalle</button>
          </a>
      </article>
    {{/each}}
  </div>
</section>